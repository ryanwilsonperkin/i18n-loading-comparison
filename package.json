  "scripts": {
    "serve-proxy": "webpack serve --config-name=proxy --mode=development --port 8080",
    "serve-en": "webpack serve --config-name=en --mode=development --port 8081",
    "serve-fr": "webpack serve --config-name=fr --mode=development --port 8082",
    "serve": "npm-run-all --parallel serve-proxy serve-en serve-fr"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
