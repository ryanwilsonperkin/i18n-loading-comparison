    "html-webpack-plugin": "^5.5.3",
    "npm-run-all": "^4.1.5",
    "react-router-dom": "^6.14.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@shopify/polaris": "^11.2.2",
    "@shopify/polaris-icons": "^7.1.0",
    "@tanstack/react-query": "^4.29.19",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-router": "^6.14.0"
  }
